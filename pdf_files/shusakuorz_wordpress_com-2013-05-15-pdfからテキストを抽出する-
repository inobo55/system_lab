	<!-- main START -->
	<div id="main">


	<div id="postpath">
		<a title="ホームページへ移動" href="http://shusakuorz.wordpress.com/">ホーム</a>
		 &gt; <a href="http://shusakuorz.wordpress.com/category/c/%e3%83%95%e3%82%a1%e3%82%a4%e3%83%ab%e6%93%8d%e4%bd%9c-c/" title="ファイル操作 の投稿をすべて表示" rel="category tag">ファイル操作</a>, <a href="http://shusakuorz.wordpress.com/category/c/%e3%83%a9%e3%82%a4%e3%83%96%e3%83%a9%e3%83%aa-c/" title="ライブラリ の投稿をすべて表示" rel="category tag">ライブラリ</a>, <a href="http://shusakuorz.wordpress.com/category/c/" title="C# の投稿をすべて表示" rel="category tag">C#</a>		 &gt; PDFからテキストを抽出する	</div>

	<div id="post-877" class="post-877 post type-post status-publish format-standard hentry category--c category--c category-c tag-itextsharp tag-microsoft-visual-studio tag-parser tag-pdf tag-streamwriter">
		<h2>PDFからテキストを抽出する</h2>
		<div class="info">
			<span class="date">2013年5月15日</span>
			<span class="author"><a href="http://shusakuorz.wordpress.com/author/shusakuorz/" title="shusaku suda による投稿" rel="author">shusaku suda</a></span>										<span class="addcomment"><a href="#respond">コメントをどうぞ</a></span>
				<span class="comments"><a href="#comments">コメントへ</a></span>
						<div class="fixed"></div>
		</div>
		<div class="content">
			<p>実行環境: <strong>Windows7 Pro</strong> / <strong>Microsoft Visual Studio 2012 Express( C# )</strong></p>
<table cellpadding="0" cellspacing="0" border="0" style="border-style:hidden;">
<tr>
<td>
<a class="thickbox" href="http://shusakuorz.files.wordpress.com/2013/05/2013-05-15.png"><img src="http://shusakuorz.files.wordpress.com/2013/05/2013-05-15.png?w=150&#038;h=106" alt="src.pdf" width="150" height="106" class="alignnone size-thumbnail wp-image-887" /></a>
</td>
<td>
<a class="thickbox" href="http://shusakuorz.files.wordpress.com/2013/05/2013-05-15-02.png"><img src="http://shusakuorz.files.wordpress.com/2013/05/2013-05-15-02.png?w=150&#038;h=112" alt="pdf2Text.txt" width="150" height="112" class="alignnone size-thumbnail wp-image-889" /></a>
</td>
<td>
<a class="thickbox" href="http://shusakuorz.files.wordpress.com/2013/05/2013-05-15-03.png"><img src="http://shusakuorz.files.wordpress.com/2013/05/2013-05-15-03.png?w=150&#038;h=112" alt="pdf2Byte.txt" width="150" height="112" class="alignnone size-thumbnail wp-image-890" /></a>
</td>
</tr>
</table>
<p><a href="http://sourceforge.net/projects/itextsharp/" title="iTextSharp" target="_blank">iTextSharp</a>を使ってPDFからテキストを抽出するサンプル。</p>
<p><span id="more-877"></span></p>
<ol>
<li><a href="http://sourceforge.net/projects/itextsharp/" title="iTextSharp" target="_blank">itextsharp-all-5.4.1.zip</a>をダウンロードする。</li>
<li>ダウンロードした圧縮ファイルを展開する。</li>
<li>Visual Studioのツールバーから<strong>プロジェクト</strong> &gt; <strong>参照の追加</strong> &gt; <strong>itextsharp.dll</strong> を追加</li>
<li>using <strong>iTextSharp.text;</strong>を記述</li>
<li>using <strong>iTextSharp.text.pdf;</strong>を記述</li>
<li>using <strong>iTextSharp.text.pdf.parser;</strong>を記述</li>
</ol>
<p>次のソースは、PDFから読み取ったテキストをテキストファイルへ書き出すサンプル。</p>
<pre class="brush: csharp; title: ; notranslate" title="">
using System.IO;
using iTextSharp.text;
using iTextSharp.text.pdf;
using iTextSharp.text.pdf.parser;

namespace Example
{
    public partial class Form1 : Form {

        #region メンバ変数
        private string pdfFileName = @&quot;C:\src1.pdf&quot;;
        private string outTextFileName = @&quot;C:\pdf2Text.txt&quot;;
        private string outByteFileName = @&quot;C:\pdf2Byte.txt&quot;;
        #endregion

        #region コンストラクタ
        public Form1()
        {
            InitializeComponent();
        }
        #endregion

        #region button3のクリックイベント
        private void button3_Click(object sender, EventArgs e)
        {
            int page;
            int i;
            int j;
            
            StreamWriter swText = null;     //StreamWriter swText = new StreamWriter(pdf2Text, false, System.Text.Encoding.GetEncoding(&quot;utf-8&quot;));
            StreamWriter swByte = null;     //StreamWriter swByte = new StreamWriter(pdf2Byte, false, System.Text.Encoding.GetEncoding(&quot;utf-8&quot;));
            PdfReader pdfReader = null;

            try
            {
                if (File.Exists(pdfFileName))
                {
                    swText = new StreamWriter(outTextFileName);
                    swByte = new StreamWriter(outByteFileName);
                    pdfReader = new PdfReader(pdfFileName);

                    ITextExtractionStrategy strategy = new SimpleTextExtractionStrategy();
                    string currentText = null;
                    string[] lines = null;
                    byte[] bytes = null;

                    // ページ単位
                    for (page = 1; page &lt;= pdfReader.NumberOfPages; page++)
                    {
                        currentText = PdfTextExtractor.GetTextFromPage(pdfReader, page, strategy);
                        lines = currentText.Split('\n');
                        // 行単位
                        for (i = 0; i &lt; lines.Length; i++)
                        {
                            // テキスト1行分を出力
                            swText.WriteLine(lines[i]);
                            // テキスト1行分をバイト表現で出力
                            bytes = Encoding.Default.GetBytes(lines[i]);
                            for (j = 0; j &lt; bytes.Length; j++) {
                                swByte.Write(&quot;{0:X2} &quot;, bytes[j]);
                            }
                            swByte.Write(swByte.NewLine);
                        }
                    }
                }
            }
            finally
            {
                // 例外が発生してもストリームのクローズと破棄を行うようにここで処理する
                if (swText != null)
                {
                    swText.Flush();
                    swText.Close();
                    swText.Dispose();
                    swText = null;
                }
                if (swByte != null)
                {
                    swByte.Flush();
                    swByte.Close();
                    swByte.Dispose();
                    swByte = null;
                }
                if (pdfReader != null)
                {
                    pdfReader.Close();
                    pdfReader.Dispose();
                    pdfReader = null;
                }
            }
        }
        #endregion
    }
}
</pre>
